{
  "name": "user-media-plug",
  "version": "0.1.0",
  "description": "servers that manage user media connections",
  "main": "./index.js",
  "scripts": {
    "preinstall": "npm run prod-url",
    "postinstall": "cd ./clientele && npm i && cd ..",
    "prod-url": "run-script-os",
    "prod-url:darwin:linux": "sudo node ./prod_url.js",
    "start": "node ./index.js",
    "bg-start": "run-script-os",
    "bg-start:win32": "start node ./index.js /b",
    "bg-start:darwin:linux": "node ./index.js > /dev/null 2>&1 &",
    "bg-kill": "run-script-os",
    "bg-kill:win32": "node ./bg_kill_win32.js",
    "bg-kill:darwin:linux": "kill $(jobs -p)",
    "pretest": "npm run bg-start",
    "test": "tape ./test.js && browserify ./clientele/test.js | tape-puppet",
    "posttest": "npm run bg-kill"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chiefbiiko/user-media-plug.git"
  },
  "now": {
    "name": "plugtube",
    "alias": "plugtube",
    "env": {
      "NODE_ENV": "production",
      "HOST": "0.0.0.0",
      "PORT": "41900",
      "DEBUG": "user-media-plug:*"
    },
    "static": {
      "public": "./view/build"
    },
    "scale": {
      "bru1": {
        "min": 1,
        "max": 1
      }
    }
  },
  "keywords": [
    "user-media",
    "chat",
    "two-party",
    "multi-party",
    "stream"
  ],
  "author": "Noah Anabiik Schwarz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chiefbiiko/user-media-plug/issues"
  },
  "homepage": "https://github.com/chiefbiiko/user-media-plug#readme",
  "devDependencies": {
    "browserify": "16.2.3",
    "tape": "4.9.0",
    "tape-puppet": "0.1.5"
  },
  "dependencies": {
    "run-script-os": "1.0.3",
    "cache-set": "0.3.0",
    "debug": "3.1.0",
    "duplex-json-stream": "1.0.1",
    "encoding-down": "5.0.4",
    "hashtag-stream-set": "github:chiefbiiko/hashtag-stream-set",
    "hooked-ttl-map": "github:chiefbiiko/hooked-ttl-map",
    "levelup": "3.1.1",
    "memdown": "3.0.0",
    "preactor": "0.7.0",
    "pump": "3.0.0",
    "stream-set": "1.1.0",
    "websocket-stream": "5.1.2"
  }
}
